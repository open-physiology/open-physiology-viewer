{
  "name": "open-physiology-viewer",
  "version": "0.1.0",
  "description": "Templates for starting new open-physiology projects",
  "main": "dist/index.js",
  "scripts": {
    "test":       "mocha-webpack --opts mocha-webpack.opts",
    "build":      "cross-env BABEL_ENV=production webpack",
    "docs":       "esdoc",
    "everything": "npm test && npm run build && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-physiology/open-physiology-viewer.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/open-physiology/open-physiology-viewer/issues"
  },
  "homepage": "https://github.com/open-physiology/open-physiology-viewer#readme",
  "devDependencies": {
    "babel-cli": "6",
    "babel-core": "6",
    "babel-loader": "6",
    "babel-plugin-espower": "2",
    "babel-plugin-syntax-flow": "6",
    "babel-plugin-transform-async-to-generator": "6",
    "babel-plugin-transform-class-properties": "6",
    "babel-plugin-transform-decorators-legacy": "1",
    "babel-plugin-transform-es2015-instanceof": "6",
    "babel-plugin-transform-flow-strip-types": "6",
    "babel-plugin-transform-function-bind": "6",
    "babel-plugin-transform-object-rest-spread": "6",
    "babel-plugin-transform-strict-mode": "6",
    "babel-plugin-typecheck": "3",
    "babel-plugin-unassert": "2",
    "babel-polyfill": "6",
    "babel-preset-es2015": "6",
    "chai": "3",
    "chai-as-promised": "5",
    "chai-match-pattern": "0.2",
    "chai-things": "0.2",
    "copy-webpack-plugin": "4",
    "cross-env": "3",
    "esdoc": "0.5",
    "esdoc-babel-plugin": "0.1",
    "file-loader": "0.11",
    "json-stringify-safe": "5",
    "mocha": "2",
    "mocha-webpack": "0.5",
    "npm-cli": "0.1",
    "polyfill-function-prototype-bind": "0.0.1",
    "power-assert": "1",
    "sinon": "1",
    "sinon-chai": "2",
    "source-map-support": "0.4",
    "url-loader": "0.5",
    "webpack": "2",
    "webpack-node-externals": "1.5"
  },
  "dependencies": {
    "utilities": "open-physiology/utilities",
    "bound-native-methods": "0.1",
    "deep-freeze-strict": "1",
    "lodash-bound": "1",
    "rxjs": "5"
  }
